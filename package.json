{
  "name": "@petesena/mcp-fathom-server",
  "version": "2.0.0",
  "description": "MCP server for Fathom AI meeting API integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-fathom-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:http": "node dist/http-server.js",
    "dev": "tsx src/index.ts",
    "dev:http": "tsx src/http-server.ts",
    "test": "npx @modelcontextprotocol/inspector dist/index.js",
    "test:http": "npx @modelcontextprotocol/inspector"
  },
  "keywords": [
    "mcp",
    "fathom",
    "meetings",
    "ai"
  ],
  "author": "petesena",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
